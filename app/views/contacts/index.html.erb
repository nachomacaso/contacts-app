<h1>Here are all your contacts:  </h1>

<% @contacts.each do |contact| %>

  <h2>Full Name: <%= link_to contact.full_name, "/contacts/#{contact.id}" %></h2>
  <h6>Updated at: <%= contact.friendly_updated_at %></h6>
  <h3>Phone: <%= contact.japanese_phone %></h3>
  <h3>Email: <%= contact.email %></h3>
<% end %>